= () => {\n    setCalendarDate((prevDate) => add(prevDate, { months: 1 }));\n  };\n\n  return (\n    <div className={styles.body}>\n      <div className={styles.header}>\n        <ArrowLeftIcon\n          onClick={prevMonth}\n          className={styles.icon}\n        />\n        <div>\n          {`${currMonth} ${currYear}`}\n        </div>\n        <ArrowRightIcon\n          onClick={nextMonth}\n          className={styles.icon}\n        />\n      </div>\n      <div className={styles.calendar}>\n        <div className={styles.dayNames}>\n          {['M', 'T', 'W', 'T', 'F', 'S', 'S'].map((day) => <div>{day}</div>)}\n        </div>\n        {\n            calendar.map((week:Date[]) => (\n              <div className={styles.days}>\n                {\n                week.map((day:Date) => (\n                  <button\n                    type=\"button\"\n                    disabled={isSelected(day)}\n                    className={styles.day}\n                    onClick={() => updateDay(day)}\n                  >\n                    <div className={dayStyles(day)}>{format(day, 'd')}</div>\n                  </button>\n                ))\n                }\n              </div>\n            ))\n        }\n      </div>\n    </div>\n  );\n};\n","/home/oadmin/projects/Clever to-do list/my-app/src/components/Calendar/build.ts",["180","181","182"],"import {\n  startOfISOWeek,\n  startOfMonth,\n  lastDayOfISOWeek,\n  lastDayOfMonth,\n  addDays,\n  isBefore,\n} from 'date-fns';\n\nexport const calendarBuild = (currentDay: Date): Date[][] => {\n  const startDay = startOfISOWeek(startOfMonth(currentDay));\n  const lastDay = lastDayOfISOWeek(lastDayOfMonth(currentDay));\n  const calendar = [];\n  let day = new Date(startDay);\n  const isEqual = require('date-fns/isEqual');\n\n  while (isBefore(day, lastDay)) {\n    calendar.push(\n      Array(7)\n        .fill(0)\n        .map(\n          (item, key) => (day = addDays(day, !key && isEqual(startDay, day) ? 0 : 1)),\n        